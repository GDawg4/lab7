<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.garoz.lab7">

    <application
            android:allowBackup="false"
            android:icon="@mipmap/ic_launcher"
            android:label="Contactos"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name="com.garoz.tagnotes.MainActivity"
                  android:launchMode="singleTop">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity android:name="com.garoz.tagnotes.AddEditContactActivity"
                  android:parentActivityName="com.garoz.tagnotes.MainActivity">

        </activity>
    </application>

</manifest>